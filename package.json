{
  "name": "words.brychta.name",
  "version": "1.0.0",
  "scripts": {
    "start": "webpack-dev-server --host 0.0.0.0 --port 3000 --hot --progress --colors --history-api-fallback",
    "build": "webpack --config webpack.production.config.js"
  },
  "babel": {
    "babelrc": false,
    "presets": [
      "es2015",
      "react",
      "stage-0"    ]
  },
  "eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "extends": [
      "standard",
      "standard-react"
    ],
    "globals": {
      "__DEV__": false,
      "__PROD__": false
    },
    "env": {
      "browser": true,
      "node": true,
      "commonjs": true,
      "es6": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ],
      "key-spacing": [
        2,
        {
          "beforeColon": true,
          "mode": "minimum",
          "align": "colon"
        }
      ],
      "no-multi-spaces": 0
    }
  },
  "devDependencies": {
    "axios": "^0.15.2",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.10.2",
    "eslint-config-standard": "^5.3.1",
    "eslint-config-standard-react": "^2.4.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-standard": "^1.3.2",
    "file-loader": "^0.8.5",
    "node-sass": "^3.8.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
