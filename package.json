{
  "name": "words.brychta.name",
  "version": "1.0.0",
  "babel": {
    "babelrc": false,
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "extends": [
      "standard"
    ],
    "globals": {
      "casper": true
    },
    "env": {
      "browser": true,
      "node": true,
      "commonjs": true,
      "es6": true,
      "jquery": true,
      "phantomjs": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ],
      "key-spacing": [
        2,
        {
          "beforeColon": true,
          "mode": "minimum",
          "align": "colon"
        }
      ],
      "no-multi-spaces": 0
    }
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "browser-sync": "^2.18.5",
    "eslint": "^3.11.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-cssretarget": "^1.1.4",
    "gulp-google-fonts": "^1.0.3",
    "gulp-sass": "^3.0.0",
    "gulp-load-plugins": "^1.4.0",
    "gulp-plumber": "^1.1.0",
    "gulp-strip-debug": "^1.1.0",
    "gulp-util": "^3.0.7",
    "gulp-babel": "^6.1.2",
    "gulp-sourcemaps": "^1.9.1"
  }
}
